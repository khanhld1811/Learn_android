<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.duykhanh.layout">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ViewMain.ControlAcitivity"></activity>
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Layout.AbsoluteLayout" />
        <activity android:name=".Layout.FrameLayout" />
        <activity android:name=".Layout.ConstraintLayout" />
        <activity android:name=".Layout.TableLayout" />
        <activity android:name=".Layout.GridLayout" />
        <activity android:name=".Layout.RelativeLayout" />
        <activity android:name=".Layout.LinearLayout" />
        <activity android:name=".ViewMain.LayoutActivity"/>
    </application>

</manifest>